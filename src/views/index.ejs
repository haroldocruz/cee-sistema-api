<!DOCTYPE html>
<html>

<head>
  <title><%= title %></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>
  <div style="text-align: center">
    <h1><%= title %></h1>
    <p>Welcome</p>
  </div>
  <div>
    <% routes.forEach(function(route){ %>
    <p>
      <%= route.description %> <br>
      <%= route.method %> <%= route.path %>
    </p>
    <% }) %>
  </div>
</body>

</html>